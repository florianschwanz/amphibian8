<?xml version='1.0' encoding='utf-8'?>
<widget id="berlin.intero.amphibian7" version="0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Amphibian</name>
  <description>
    A sample Apache Cordova application that responds to the deviceready event.
  </description>
  <author email="thinktank@interoberlin.de" href="https://interoberlin.de">
    Interoberlin
  </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <hook src="scripts/prepare-app.js" type="before_prepare"/>
  <preference name="SplashShowOnlyFirstTime" value="false"/>
  <preference name="ShowSplashScreenSpinner" value="false"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <icon density="xxhdpi" src="www/assets/icons/icon-144x144.png"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <engine name="android" spec="~7.1.4"/>
</widget>
